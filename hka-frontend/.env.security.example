# Example secure environment file (copy to .env.local and fill values)
# This file intentionally leaves sensitive values blank.
# Do NOT commit real secrets.

# Private deployer key (DO NOT USE IN FRONTEND IN PRODUCTION)
PRIVATE_KEY=

# Backend-only RPC endpoints (frontend should rely on VITE_ prefixed public ones or a proxy)
RPC_ETHEREUM_SEPOLIA=
RPC_ARBITRUM_SEPOLIA=
RPC_OPTIMISM_SEPOLIA=
RPC_BASE_SEPOLIA=

# Public-ish keys (still treat as sensitive if tied to quota)
VITE_WALLETCONNECT_PROJECT_ID=
VITE_INFURA_API_KEY=
VITE_ALCHEMY_API_KEY=

# Seeding parameters (local dev only)
SEED_ETH=0.05
SEED_USDC=150

# Feature flags
VITE_DEBUG_DEX=false
FEATURE_CCPAYLOAD=false

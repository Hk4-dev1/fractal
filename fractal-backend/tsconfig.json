{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"types": ["node", "hardhat"],
		"outDir": "dist"
	},
	"include": ["./scripts", "./test", "./hardhat.config.ts"],
	"exclude": ["node_modules"]
}
